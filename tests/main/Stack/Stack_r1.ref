REFINEMENT
    Stack_r1
REFINES
    Stack

SEES b2rust_types

ABSTRACT_VARIABLES
    Contents_r1

INVARIANT
    Contents_r1 : seq ( rust_i8 ) &
    Contents = rev ( Contents_r1 )

INITIALISATION
    Contents_r1 := <>

OPERATIONS

    push ( value ) =
    BEGIN
        Contents_r1 := Contents_r1 <- value
    END
    ;

    value <-- pop =
    BEGIN
        value := last ( Contents_r1 ) ||
        Contents_r1 := front ( Contents_r1 )
    END

ASSERTIONS
    Contents_r1 = rev ( Contents ) ;
    size ( Contents_r1 ) <= 10

END
