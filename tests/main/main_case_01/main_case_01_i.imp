IMPLEMENTATION main_case_01_i

REFINES main_case_01

SEES
    b2rust_types

OPERATIONS

    res <-- meilleur ( jour ) =
    BEGIN
        CASE jour OF
            EITHER 6  THEN res := TRUE
            OR     7  THEN res := TRUE
            ELSE res := FALSE
        END
    END

 END

END
