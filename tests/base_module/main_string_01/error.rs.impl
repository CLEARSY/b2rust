use std::convert::TryFrom;

pub struct error {}

impl error {
    pub fn new(id: usize) -> Self {
        let mut instance = Self {};
        instance.initialisation();
        instance
    }

    fn initialisation(&mut self) {}

    pub fn error_msg(&mut self, r#message: &str) {
        println!("{}",r#message);
    }
}



