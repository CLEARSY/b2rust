IMPLEMENTATION expression_01_i
REFINES expression_01
    
    SEES
    b2rust_types

INVARIANT
    value : rust_i32
    
INITIALISATION
    value := 0
    
OPERATIONS
    adding = BEGIN
        IF value = 12 THEN
            value := value * 200;
            value := value - 70000;
            value := value + 67600
            END
    END

END